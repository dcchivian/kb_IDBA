[run]
branch = True

omit =
    /kb/module/lib/kb_IDBA/authclient.py
    /kb/module/lib/kb_IDBA/baseclient.py
    /kb/module/lib/kb_IDBA/kb_IDBAClient.py
    /kb/module/lib/kb_IDBA/kb_IDBAServer.py
source = /kb/module/lib/kb_IDBA


[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError

show_missing = 1
